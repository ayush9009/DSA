#include <bits/stdc++.h>
using namespace std;
#define ll long long int
void thrill(){
    ll n, m;
    cin>>m>>n;
    ll cnt=0;
    while(m%2==0){
        m/=2;
        cnt++;
    }
    if(n%m!=0 || n/m > (1LL<<cnt)){
        cout<<"NO"<<endl;
        return;
    }
    cout<<"YES"<<endl;
}
int main(){
    int t;
    cin>>t;
    while(t--){
        thrill();
    }
return 0;

}
